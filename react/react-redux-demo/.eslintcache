[{"/Users/lius/Desktop/software/web-learning/react/react-redux-demo/src/index.js":"1","/Users/lius/Desktop/software/web-learning/react/react-redux-demo/src/TodoList.js":"2","/Users/lius/Desktop/software/web-learning/react/react-redux-demo/src/store/index.js":"3","/Users/lius/Desktop/software/web-learning/react/react-redux-demo/src/store/reducer.js":"4"},{"size":520,"mtime":1624973951933,"results":"5","hashOfConfig":"6"},{"size":1730,"mtime":1626615492546,"results":"7","hashOfConfig":"6"},{"size":232,"mtime":1624973951933,"results":"8","hashOfConfig":"6"},{"size":633,"mtime":1626615460573,"results":"9","hashOfConfig":"6"},{"filePath":"10","messages":"11","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"12"},"aief06",{"filePath":"13","messages":"14","errorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":"15","usedDeprecatedRules":"12"},{"filePath":"16","messages":"17","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"12"},{"filePath":"18","messages":"19","errorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":"20"},"/Users/lius/Desktop/software/web-learning/react/react-redux-demo/src/index.js",[],["21","22"],"/Users/lius/Desktop/software/web-learning/react/react-redux-demo/src/TodoList.js",["23"],"import React, { Component } from \"react\";\nimport { Input, Button, List } from \"antd\";\nimport DragResizeDialog from 'react-drag-resize-dialog'\nimport 'react-drag-resize-dialog/lib/min.min.css'\nimport { connect } from 'react-redux'\n\nconst boxStyle = {\n  width: 500,\n  margin: '10px auto',\n  boxShadow: '10px 5px 5px #bfb7b7,-10px 5px 5px #bfb7b7',\n  padding: 10\n}\nclass TodoList extends Component {\n\n  state = {\n    visible: false\n  }\n\n  clickBtn = () => {\n    this.props.addTodo(this.props.inputValue)\n  }\n\n  render() {\n    return (\n      <div style={boxStyle}>\n        <h2>Redux</h2>\n        <div>\n          <Input\n            onChange={this.props.inputChange}\n            value={this.props.inputValue}\n            style={{ width: 250, marginRight: 10 }}\n          />\n          <Button\n            type=\"primary\"\n            onClick={this.clickBtn}\n          >增加</Button>\n        </div>\n        <div style={{ margin: 10, width: 300 }}>\n          <List\n            bordered\n            dataSource={this.props.list}\n            renderItem={(item, index) => <List.Item extra={<Button type='primary' onClick={() => {this.props.delTodo(index) }}>删除</Button>}>{item}</List.Item>}\n          />\n        </div>\n      </div>\n    )\n  }\n}\n\nconst mapstateToProps = state => ({\n  ...state\n})\n\nconst mapDispatchToProps = (dispatch) => ({\n  inputChange: e => {\n    const action = {\n      type: 'input_change',\n      value: e.target.value\n    }\n    dispatch(action)\n  },\n  addTodo: v => {\n    dispatch({\n      type: 'add',\n      value: v\n    })\n  },\n  delTodo: index => {\n    dispatch({\n      type: 'delete',\n      value: index\n    })\n  }\n})\n\nexport default connect(mapstateToProps, mapDispatchToProps)(TodoList)\n// export default TodoList","/Users/lius/Desktop/software/web-learning/react/react-redux-demo/src/store/index.js",[],"/Users/lius/Desktop/software/web-learning/react/react-redux-demo/src/store/reducer.js",["24"],"const defaultState = {\n  inputValue: \"XXX\",\n  list: [\"早8点开晨会\", \"早9点开晨会,需求沟通会\", \"早8点开晨会\", \"早8点开晨会\"],\n}\n\nexport default (state = defaultState, action) => {\n  if (action.type === 'input_change') {\n    return {\n      ...state,\n      inputValue: action.value\n    }\n\n  }\n  if (action.type === 'add') {\n    const newState = JSON.parse(JSON.stringify(state))\n    newState.list.push(action.value)\n    return newState\n  }\n\n  if (action.type === 'delete') {\n    let newState = JSON.parse(JSON.stringify(state));\n    newState.list.splice(action.value, 1)\n    return newState\n  }\n  return state\n}",{"ruleId":"25","replacedBy":"26"},{"ruleId":"27","replacedBy":"28"},{"ruleId":"29","severity":1,"message":"30","line":3,"column":8,"nodeType":"31","messageId":"32","endLine":3,"endColumn":24},{"ruleId":"33","severity":1,"message":"34","line":6,"column":1,"nodeType":"35","endLine":26,"endColumn":2},"no-native-reassign",["36"],"no-negated-in-lhs",["37"],"no-unused-vars","'DragResizeDialog' is defined but never used.","Identifier","unusedVar","import/no-anonymous-default-export","Assign arrow function to a variable before exporting as module default","ExportDefaultDeclaration","no-global-assign","no-unsafe-negation"]